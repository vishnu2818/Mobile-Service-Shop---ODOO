<?xml version="1.0" encoding="utf-8"?>
<odoo>
    <data>
        <!-- category -->
        <record id="module_mobile_shop_category" model="ir.module.category">
            <field name="name">Mobile Service Shop</field>
            <field name="sequence">0</field>
            <field name="description">Mobile Service Shop</field>
        </record>

        <!-- group-1 -->
        <record id="group_mobile_shop_user" model="res.groups">
            <field name="name">MobileShop / User</field>
            <field name="category_id" ref="module_mobile_shop_category"/>
        </record>
        <!-- group-2 -->
        <record id="group_mobile_shop_manager" model="res.groups">
            <field name="name">MobileShop / Manager</field>
            <field name="category_id" ref="module_mobile_shop_category"/>
        </record>
    </data>
</odoo>









